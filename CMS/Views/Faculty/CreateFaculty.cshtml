@using CMS.Models
@model Faculty
@{
    Layout = "~/Views/Shared/_LayoutUsers.cshtml";
    ViewBag.Title = "CreateFaculty";
}

<h2>Create</h2>

@using (Html.BeginForm("Create", "Faculty", FormMethod.Post, new { @class = "form-horizontal" }))
{ <div></div>
    <div class="form-group">
        @Html.Label("Facutly", new { @class = "col-md-2 control-label" })
        @Html.TextBoxFor(m => m.Name, new { @class = "col-md-8 form-control", placeholder = "Faculty..." })
    </div>
    <div class="form-group">
        <div class="col-md-offset-2">
            <input type="submit" class="btn btn-default" value="Create" />
        </div>
    </div>


}
<ul class="pager">
    <li class="previous"><a href="~/Faculty/"><span aria-hidden="true">&larr;</span> Back</a></li>
</ul>

